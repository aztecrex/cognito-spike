{
  "name": "cognito-spike",
  "version": "0.1.0",
  "private": true,
  "bin": {
    "cognito-spike": "out/bin/cognito-spike.js"
  },
  "scripts": {
    "build": "npm run clean && ./build.sh",
    "deploy": "./build.sh && cdk deploy --require-approval never '*'",
    "diff": "./build.sh && cdk diff",
