    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "web": "npx browser-sync start --server dist/www/ --files dist/www/"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.19.0",
    "@types/aws-lambda": "^8.10.39",
    "@types/cookie": "^0.3.3",
    "@types/jest": "^24.0.25",
    "@types/node": "^12.12.24",
    "@types/node-fetch": "^2.5.4",
    "aws-cdk": "^1.19.0",
    "browser-sync": "^2.26.7",
    "jest": "^24.9.0",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "^1.19.0",
    "@aws-cdk/aws-cloudfront": "^1.19.0",
    "@aws-cdk/aws-cognito": "^1.19.0",
    "@aws-cdk/aws-iam": "^1.19.0",
    "@aws-cdk/aws-s3": "^1.19.0",
    "@aws-cdk/core": "^1.19.0",
    "amazon-cognito-identity-js": "^3.2.2",
    "aws-lambda": "^1.0.4",
    "aws-sdk": "^2.597.0",
    "cookie": "^0.4.0",
    "dotenv": "^8.2.0",
    "form-data": "^3.0.0",
    "history": "^4.10.1",
